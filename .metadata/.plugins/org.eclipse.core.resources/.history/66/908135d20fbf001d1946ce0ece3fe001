package com.masai;

import java.io.File;

public class FileDirDemo {
	public static void main(String[] args) {
	
		File file = new File("MyDir");
		
		if(!file.exists()) {
			
			System.out.println("No Directory name MyDir");
			return;
		}
		
		System.out.println("MyDir is a directory "+file.isDirectory());
		
		
	}

}
